{
  "name": "grpc-web-ui",
  "version": "0.0.0",
  "description": "Web UI for interacting with gRPC services",
  "main": "index.js",
  "scripts": {
    "audit": "npm audit",
    "build": "next build",
    "dev": "node index.js",
    "lint": "standard | snazzy",
    "start": "NODE_ENV=production node index.js",
    "test": "npm run build && NODE_ENV=test jest --env=node",
    "test-changes": "npm run build && NODE_ENV=test jest --env=node --watch --no-coverage",
    "watch-test": "npm run build && NODE_ENV=test jest --env=node --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntonRublev360/grpc-web-ui.git"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "keywords": [
    "grpc",
    "web",
    "ui",
    "javascript"
  ],
  "author": "anton.v.rublev@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AntonRublev360/grpc-web-ui/issues"
  },
  "homepage": "https://github.com/AntonRublev360/grpc-web-ui#readme",
  "devDependencies": {
    "jest": "^23.5.0",
    "pre-commit": "^1.2.2",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "next": "^6.1.1",
    "rc": "^1.2.8",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "serve-favicon": "^2.5.0"
  },
  "pre-commit": [
    "lint",
    "test",
    "audit"
  ]
}
